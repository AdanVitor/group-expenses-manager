<% include ../header %>
	<header>
		<h2>Ntalk - Dados do contact</h2>
	</header>
	<section>
	<form action="/contact/<%- contact._id %>?_method=delete"method="post">
		<p><label>Nome:</label><%- contact.name %></p>
		<p><label>E-mail:</label><%- contact.email %></p>
		<p>
			<button type="submit">Excluir</button>
			<a href="/contact/<%- contact._id %>/editar">Editar</a>
		</p>
	</form>
</section>
<% include ../exit %>
<% include ../footer %>